{
  "name": "LetVibe",
  "version": "0.0.0",
  "private": true,
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "scripts": {
    "start": "node ./build/app.js",
    "clean": "rm -rf build && mkdir -p build/config",
    "build-server": "babel -d ./build/config ./config && babel -d ./build ./app.js",
    "build-front": "rsync -av --progress ./front ./build --exclude public/js && mkdir ./build/front/public/js && babel -d ./build/front/public/js ./front/public/js"
  },
  "dependencies": {
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "node-sass-middleware": "0.8.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0"
  }
}
